programa {
	funcao inicio() {
	    // operadores aritméticos
	    
		inteiro x = 6
		inteiro y = 2
		
		// 6 + 2 = 8
		escreva(x+y)
		// quebra de linha
		escreva("\n")
		//6 - 2 = 4
		escreva(x-y)
		// quebra de linha
		escreva("\n")
		// 6 x 2 = 12
		escreva(x*y)
		// quebra de linha
		escreva("\n")
		// 6 / 2 = 3
		escreva(x/y)
		// quebra de linha
		escreva("\n")
		// o resto da divisão entre 6 e 2 é 0
		escreva(x%y)
		
	}
}
