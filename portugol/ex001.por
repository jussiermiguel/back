programa {
  funcao inicio() {
    // declaração das variáveis
    inteiro dividendo 
    inteiro divisor
    real resultado

    // entrada dos dados do usuário
    escreva("Digite o dividendo: ")
    leia(dividendo)

    escreva("Digite o divisor: ")
    leia(divisor)

    // processamento dos dados
    resultado = dividendo / divisor

    // impressão do resultado
    escreva(resultado)

  }
}
